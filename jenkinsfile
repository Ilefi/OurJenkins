pipeline {
    agent any 

    stages {
        stage('Checkout Code') {
            steps {
                git url: 'https://github.com/Ilefi/OurJenkins.git',
                    branch: 'main' // J'ai utilisé 'main' car le log montre la vérification de 'main'
            }
        }
        stage('Build') {
            steps {
                echo 'Starting the Build Stage...'
                // Ceci va utiliser l'outil Maven configuré dans Jenkins
                sh 'mvn clean install'
            }
        }
    }
}